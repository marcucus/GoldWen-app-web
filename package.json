{
  "name": "goldwen-showcase",
  "version": "0.0.1",
  "description": "GoldWen dating app showcase website built with Next.js and Tailwind CSS",
  "author": "GoldWen Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "build:css": "npx tailwindcss -i ./styles/globals.css -o ./public/css/styles.css --watch=false",
    "watch:css": "npx tailwindcss -i ./styles/globals.css -o ./public/css/styles.css --watch"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.2.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.3"
  }
}
